<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#f75231"
                android:endColor="#faa"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#a51b00" />
        </shape>
    </item>
    <item android:left="15dp" android:right="15dp" android:bottom="1dp" android:top="1dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="#ffc631"
                android:endColor="#ffdb7e"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#a51b00" />
        </shape>
    </item>
</layer-list>