<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#ada594"
                android:endColor="#9d9481"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#766b54" />
        </shape>
    </item>
    <item android:left="15dp" android:right="15dp" android:bottom="1dp" android:top="1dp">
        <shape android:shape="oval">
            <gradient
                android:startColor="#fff"
                android:endColor="#c6c6bd"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#766b54" />
        </shape>
    </item>
</layer-list>